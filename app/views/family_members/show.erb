<div class="flex-index">
    <h1 class="h1-body"><% @family_member.first_name %> <% @family_member.last_name %><a href="/family_member/<%= @family_member.id %>/edit"> Edit</a></h1>

    <div class="plan-box">
        <% @plans.each do |plan| %>
            <h3><%= plan.company %></h3>
            <h4><%= plan.type %></h4>
            <a class="plan-button" href="/plan/#{plan.id}">View Plan</a>
        <% end %>
    </div>
</div>